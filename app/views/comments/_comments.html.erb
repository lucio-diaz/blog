<div id="comment-list">
  <h3>Comments</h3>
  <div id="individual-comment">
    <% @post.comments.reverse.each do |comment| %>
    <% if comment.commenter %> 
    <%= render partial: 'comments/comment', locals: { commenter: comment.commenter, body: comment.body }  %>
    <% end %>
    <% end %>
  </div>
</div>
